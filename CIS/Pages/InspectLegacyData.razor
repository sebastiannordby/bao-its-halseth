                        <RadzenDataGridColumn TItem="Vareinfo" Title="@nameof(Vareinfo.Vgr1)" Property="@nameof(Vareinfo.Vgr1)" />
                        <RadzenDataGridColumn TItem="Vareinfo" Title="@nameof(Vareinfo.Vgr2)" Property="@nameof(Vareinfo.Vgr2)" />
                    </Columns>
                </RadzenDataGrid>
